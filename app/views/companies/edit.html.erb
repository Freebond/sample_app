<h1>My company</h1>

<%= form_for(@company) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :duns %><br />
    <%= f.text_field :duns %>
  </div>
  <div class="field">
    <%= f.label :address %><br />
    <%= f.password_field :address %>
  </div>
  <div class="field">
    <%= f.label :city %><br />
    <%= f.password_field :city %>
  </div>
  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

